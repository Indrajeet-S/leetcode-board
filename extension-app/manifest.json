{
  "manifest_version": 3,
  "name": "LeetCode Problem Redirector",
  "version": "1.0",
  "description": "Redirects to a specific whiteboard page for LeetCode problems.",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    // "default_popup": "popup.html"
  //   "default_icon": {
  //     "16": "icons/icon16.png",
  //     "48": "icons/icon48.png",
  //     "128": "icons/icon128.png"
  //   }
  },
  "host_permissions": [
    "*://leetcode.com/*"
  ]
}
